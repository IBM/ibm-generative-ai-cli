{
  "name": "@ibm-generative-ai/cli",
  "version": "2.0.1",
  "description": "CLI for IBM Generative AI (Tech Preview)",
  "keywords": [
    "ai",
    "generative",
    "genai",
    "ibm",
    "cli"
  ],
  "homepage": "https://bam.res.ibm.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IBM/ibm-generative-ai-cli.git"
  },
  "bugs": {
    "url": "https://github.com/IBM/ibm-generative-ai-cli/issues"
  },
  "type": "module",
  "bin": {
    "genai": "./src/cli.js"
  },
  "files": [
    "src/**/*"
  ],
  "scripts": {
    "start": "./src/cli.js",
    "lint": "eslint",
    "prepare": "husky install"
  },
  "packageManager": "yarn@3.5.0",
  "engines": {
    "node": ">=18.18.2"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ibm-generative-ai/node-sdk": "^2.0.2",
    "dayjs": "^1.11.9",
    "lodash": "^4.17.21",
    "stream-json": "^1.7.5",
    "yaml": "^2.3.0",
    "yargs": "^17.7.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^8.0.0",
    "prettier": "^2.8.8"
  }
}
